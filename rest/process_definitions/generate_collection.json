{
    "id": "generate_collection",
    "summary": "Generate a synthetic collection",
    "description": "Instead of loading a collection (which is what `load_collection` does), this process creates a synthetic collection from supplied data.",
    "categories": [
        "development"
    ],
    "parameter_order": ["data", "dims", "coords"],
    "parameters": {
        "data": {
            "description": "Contents of the generated data-cube as multi-dimensional array. The size of dimensions must match the number of coords.",
            "schema": {
                "type": "array",
                "examples": [
                    [
                        [
                            [
                                [0.25, 0.15],
                                [0.15, 0.25]
                            ],
                            [
                                [0.58, 0.22],
                                [null, null]
                            ]
                        ]
                    ]
                ]
            },
            "required": true
        },
        "dims": {
            "description": "List of dimensions.",
            "schema": {
                "type": "array",
                "minItems": 1,
                "items": {
                    "type": "string"
                },
                "examples": [
                    ["x", "y", "band", "t"]
                ]
            },
            "required": true
        },
        "coords": {
            "description": "Coordinates (typically times, latitudes, longitudes and band names).",
            "schema": {
                "type": "object"
            },
            "examples": [
                {
                    "x": [12.3],
                    "y": [45.1, 45.2],
                    "t": ["2019-08-01 11:00:12", "2019-08-02 13:00:12"],
                    "band": ["nir", "red"]
                }
            ],
            "required": true
        }
    },
    "returns": {
        "description": "The generated data-cube.",
        "schema": {
            "type": "object",
            "format": "raster-cube"
        }
    },
    "examples": [
        {
            "arguments": {
                "data": [
                    [
                        [
                            [0.25, 0.15],
                            [0.15, 0.25]
                        ],
                        [
                            [0.58, 0.22],
                            [null, null]
                        ]
                    ]
                ],
                "dims": ["x", "y", "t", "band"],
                "coords": {
                    "x": [12.3],
                    "y": [45.1, 45.2],
                    "t": ["2019-08-01 11:00:12", "2019-08-02 13:00:12"],
                    "band": ["nir", "red"]
                }
            }
        }
    ]
}